#!/bin/bash

export SWT_GTK3=0
export UBUNTU_MENUPROXY=0

BASE_DIR=${PWD}
export JDK_HOME=$BASE_DIR/jre
# export JDK_HOME=$BASE_DIR/jdk --if JDK is used instead of JRE
export JAVA_HOME=$BASE_DIR/jre
# export JAVA_HOME=$BASE_DIR/jdk/jre --if JDK is used instead of JRE
export PATH=$PATH:$JAVA_HOME/bin
java -Xms128m -Xmx1024m -cp $BASE_DIR/bin/lib/*:$BASE_DIR/bin/jdbc-drivers/*:. ro.nextreports.designer.Main
